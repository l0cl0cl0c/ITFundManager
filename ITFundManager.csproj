<Project Sdk="Microsoft.NET.Sdk">
	


	<PropertyGroup>
		<TargetFrameworks>net9.0-windows10.0.19041.0</TargetFrameworks>
		<OutputType>WinExe</OutputType>
		<UseMaui>true</UseMaui>
		<SingleProject>true</SingleProject>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<WindowsPackageType>None</WindowsPackageType>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="CommunityToolkit.Maui" Version="9.0.0" />
		<PackageReference Include="CommunityToolkit.Mvvm" Version="8.4.0" />
		<PackageReference Include="sqlite-net-pcl" Version="1.9.172" />
	</ItemGroup>

	<ItemGroup>
		<MauiXaml Update="Views\ExpenseProposalHistoryPage.xaml">
		  <Generator>MSBuild:Compile</Generator>
		</MauiXaml>
		<MauiXaml Update="Views\LoginPage.xaml" />
		<MauiXaml Update="Views\FundListPage.xaml" />
		<MauiXaml Update="Views\AddFundPage.xaml" />
		<MauiXaml Update="Views\ExpenseProposalListPage.xaml" />
		<MauiXaml Update="Views\AddExpenseProposalPage.xaml" />
		<MauiXaml Update="Views\UsersPage.xaml" />
		<MauiXaml Update="Views\AddEditUserPage.xaml" />
		
		
	</ItemGroup>
	


</Project>
